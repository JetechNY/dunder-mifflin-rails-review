<h1> Edit Employee </h1>

<%= form_for @employee do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    
    <%= f.label :alias %>
    <%= f.text_field :alias %>
  
    <%= f.label :title %>
    <%= f.number_field :title%>

    <%= f.label :dog_id %>
    <%= f.collection_select :dog_id, Dog.all, :id, :name %>
    
  
    <%= f.submit %>
  <% end %>